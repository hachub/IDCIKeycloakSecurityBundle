{
    "name": "idci/keycloak-security-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily handle you application security thanks to keycloak.",
    "license": [
        "GPL-3.0-or-later",
        "CECILL-2.0"
    ],
    "keywords": [],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Benjamin Boudier",
            "email": "benjamin.boudier@idci-consulting.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^5.2",
        "symfony/framework-bundle": "5.2.*",
        "symfony/routing": "^5.2",
        "symfony/http-foundation": "^5.2.1",
        "knpuniversity/oauth2-client-bundle": "^2.8",
        "guzzlehttp/guzzle": "^7.3.0",
        "symfony/security-bundle": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.4",
        "symfony/expression-language": "^5.2.4",
        "symfony/finder": "^5.2.4",
        "symfony/form": "^5.2.6",
        "symfony/stopwatch": "^5.2.4",
        "symfony/twig-bundle": "*",
        "symfony/validator": "^5.2.6",
        "symfony/yaml": "^5.2.5"
    },
    "autoload": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\Tests\\": "Tests"
        }
    }
}
